<div class="row mb-3">
  <div class="col-md-12">
    <h1>
      All contentidos
    </h1>

    <a href="/contentidos/new" class="btn btn-block btn-outline-secondary">
      Add a new contentido
    </a>
  </div>
</div>

<!-- Ransack Code start -->

<!-- Ransack Code end -->

<!-- Location Map Code start -->

<!-- Location Map Code end -->

<hr>

<div class="row mb-3">
  <div class="col-md-12">
    <table class="table">
      <tr>
        <th>
          ID
        </th>

        <th>
          Eje
        </th>

        <th>
          Created at
        </th>

        <th>
          Updated at
        </th>

        <th>
        </th>
      </tr>

      <% @contentidos.each do |contentido| %>
      <tr>
        <!-- Display contentido.id start -->
        <td>
          <%= contentido.id %>
        </td>
        <!-- Display contentido.id end -->

        <!-- Display contentido.eje_id start -->
        <td>
          <%= contentido.eje.created_at %>
        </td>
        <!-- Display contentido.eje_id end -->

        <!-- Display contentido.created_at start -->
        <td>
          <%= time_ago_in_words(contentido.created_at) %> ago
        </td>
        <!-- Display contentido.created_at end -->

        <!-- Display contentido.updated_at start -->
        <td>
          <%= time_ago_in_words(contentido.updated_at) %> ago
        </td>
        <!-- Display contentido.updated_at end -->

        <td>
          <a href="/contentidos/<%= contentido.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
