<div class="row mb-3">
  <div class="col-md-8 offset-md-2">
    <h1>
      Creador #<%= @creador.id %> details
    </h1>

    <div class="row mb-3">
      <div class="col">
        <a href="/creadors" class="btn btn-block btn-outline-secondary">
          Go back
        </a>
      </div>

      <!-- Edit link creador start -->
      <div class="col">
        <a href="/creadors/<%= @creador.id %>/edit" class="btn btn-block btn-outline-secondary">
          Edit creador
        </a>
      </div>
      <!-- Edit link creador end -->

    </div>

    <dl>
      <dt>
        Email
      </dt>
      <!-- Display email start -->
      <dd>
        <%= @creador.email %>
      </dd>
      <!-- Display email end -->

      <dt>
        Password
      </dt>
      <!-- Display password start -->
      <dd>
        <%= @creador.password %>
      </dd>
      <!-- Display password end -->

      <dt>
        Created at
      </dt>
      <!-- Display created_at start -->
      <dd>
        <%= time_ago_in_words(@creador.created_at) %> ago
      </dd>
      <!-- Display created_at end -->

      <dt>
        Updated at
      </dt>
      <!-- Display updated_at start -->
      <dd>
        <%= time_ago_in_words(@creador.updated_at) %> ago
      </dd>
      <!-- Display updated_at end -->
    </dl>
  </div>
</div>

<!-- Show Page Customization starts -->
<div class="row mb-3">
  <div class="col-md-8 offset-md-2">
    <%= render partial: "creador_templates/creador_ejercicios" %>
  </div>
</div>
<!-- Show Page Customization end -->
